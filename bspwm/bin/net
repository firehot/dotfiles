# Quick check for current networking
(nmcli -p c && echo && nmcli dev wifi) || netctl-auto list
echo 
echo "Current enabled services:"
echo "-------------------------"
systemctl list-unit-files | grep -n enabled
echo
ping -c 5 google.com

